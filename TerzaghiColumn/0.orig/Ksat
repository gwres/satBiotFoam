FoamFile
{
    version     2.0;
    format      ascii;
    class       volTensorField;
    location    "0";
    object      Ksat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];
internalField   uniform (1e-5 0 0 0 1e-5 0 0 0 1e-5);


boundaryField
{    
    bottom
    {
        type            zeroGradient;
    }
    top
    {
        type			zeroGradient;
    }
    fixed
    {
        type            empty;
    }
}


